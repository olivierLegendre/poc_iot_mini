# Copy to .env and set values (do NOT commit .env)
TZ=Europe/Paris

# Host naming for Basics Station TLS (must match gateway LNS URI host)
LNS_HOST=poc-gw.local
LNS_PORT=3000

# LAN devices
SLZB06_IP=192.168.1.50
SLZB06_PORT=6638
RAK_GW_IP=192.168.1.60

# MQTT (Mosquitto)
MQTT_HOST=mosquitto
MQTT_PORT=1883
MQTT_ADMIN_USER=admin
MQTT_ADMIN_PASS=change_me_admin
MQTT_INGEST_USER=ingest
MQTT_INGEST_PASS=change_me_ingest
MQTT_CONTROL_USER=control
MQTT_CONTROL_PASS=change_me_control

# Zigbee2MQTT image tag
ZIGBEE2MQTT_TAG=1.40.2

# Optional: enable Zigbee2MQTT + simulator services
# COMPOSE_PROFILES=zigbee
COMPOSE_PROFILES=

# PostgreSQL
PG_HOST=postgres
PG_PORT=5432
PG_DB=chirpstack
PG_USER=chirpstack
PG_PASSWORD=change_me_pg

# ChirpStack admin (UI login)
CHIRPSTACK_ADMIN_EMAIL=admin@example.com
CHIRPSTACK_ADMIN_PASSWORD=change_me_chirpstack

# Node-RED admin (optional)
NODERED_USER=admin
NODERED_PASSWORD=change_me_nodered
